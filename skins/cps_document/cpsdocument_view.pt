<tal:block define="doc here/getContent;
                   rendered_main python: doc.render(proxy=here);
  ">
<metal:block define-macro="view">
<metal:block use-macro="here/content_lib/macros/master">
  <metal:block fill-slot="header">
  </metal:block>

  <metal:block fill-slot="css_slot">
    <link rel="Stylesheet" type="text/css" href=""
      tal:attributes="href string:${base_url}document.css">
  </metal:block>

  <metal:block fill-slot="main">
    <tal:block tal:content="structure rendered_main" />
    <div style="clear:both; text-align:right; border-top: solid 1px;">
      <small>
        <div metal:use-macro="here/content_lib/macros/byline">By Me</div>
      </small>
    </div>
  </metal:block>

</metal:block>
</metal:block>
</tal:block>
